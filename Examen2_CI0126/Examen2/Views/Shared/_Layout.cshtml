    ﻿
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title CONCESIONARIO</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Examen2</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Examen2.styles.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/boxicons@2.0.9/css/boxicons.min.css" rel="stylesheet">
</head>
<body>
    <header id="header" class="fixed-top d-flex align-items-centert">
        <div class="container d-flex justify-content-between align-items-center">
            <div class="logo">
                <h1 class="text-light"><a href="~/Home/Inicio"><span>CONCESIONARIO</span></a></h1>
            </div>
            <nav id="navbar" class="navbar">
                <ul>
                    <li>@Html.ActionLink("Administrar Carros", "", "Home")</li>
                    <li>@Html.ActionLink("Agregar Carro", "", "Home")</li>
                    
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav>
        </div>
    </header>
    <main id="main">
        <div class="container body-content" id="page-container">
         <div id="content-wrap">
                @RenderBody()
        </div>
    </div>
        <div class="container">
            <div class="copyright">
                &copy; Copyright <strong><span> @DateTime.Now.Year - Andres Matarrita</span></strong>.
            </div>
        </div>
        </main>
        <script src="/Content/vendor/aos/aos.js"></script>
        <script src="/Content/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="/Content/vendor/glightbox/js/glightbox.min.js"></script>
        <script src="/Content/vendor/isotope-layout/isotope.pkgd.min.js"></script>
        <script src="/Content/vendor/php-email-form/validate.js"></script>
        <script src="/Content/vendor/purecounter/purecounter.js"></script>
        <script src="/Content/vendor/swiper/swiper-bundle.min.js"></script>
        <script src="/Content/vendor/waypoints/noframework.waypoints.js"></script>

        @RenderSection("scripts", required: false)
</body>
</html>

